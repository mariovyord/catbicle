	<p class="name">{{name}}</p>
	<p><span>Stars:</span> {{stars}}</p>
	<a class="btn" href="/details/{{id}}">Details</a>
	<button class="btn more">Show more</button>
	<div style="display: none" class="description">
		<p>{{description}}</p>
	</div>
</div>